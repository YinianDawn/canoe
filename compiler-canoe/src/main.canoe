testLoop :{} :() := () {
    i = 0
    loop {
        i = i + 1
        if i > 5 {
            break
        }
    }

    i = 0
    loop {
        i = i + 1
        if i > 5 {
            break 
        }

        loop :loop2 {
            break loop2
        }
    }

    i = 0
    loop :loop1 {
        i = i + 1
        if i > 5 {
            break 
        }

        loop {
            continue loop1
        }
    }
}

